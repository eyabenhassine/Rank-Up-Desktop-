<VBox prefHeight="634.0" prefWidth="1227.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rankup.controllers.AfficherSponsor">

    <children>
        <VBox alignment="CENTER" spacing="10">
            <Text styleClass="title" text="SPONSORS list" />
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="800.0">
                <columns>
                    <TableColumn fx:id="NomSponsorCol" prefWidth="200.0" text="Sponsor Name" />
                    <TableColumn fx:id="AdresseSponsorCol" prefWidth="200.0" text="Address" />
                    <TableColumn fx:id="MailSponsorCol" prefWidth="200.0" text="Email" />
                </columns>
            </TableView>
            <HBox spacing="10">


                <Button mnemonicParsing="false" onAction="#naviguerAddS" text="Add" />
                <Button mnemonicParsing="false" onAction="#modifierSponsor" prefHeight="25.0" prefWidth="72.0" text="Update" />
                <Button mnemonicParsing="false" onAction="#deleteSponsor" prefHeight="26.0" prefWidth="72.0" text="Delete" />

            </HBox>
        </VBox>

        <ImageView fitHeight="150.0" fitWidth="200.0">
            <!-- Image not specified -->
        </ImageView>
    </children>
</VBox>