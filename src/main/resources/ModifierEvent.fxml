<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="tn.esprit.controllers.ModifierEvent"
            prefHeight="400.0" prefWidth="600.0"
>






        <children>
            <TextField fx:id="nomEventUpdate" layoutX="100" layoutY="20" promptText="Nom de l'événement"/>
            <TextField fx:id="dateDebutUpdate" layoutX="100" layoutY="60" promptText="Date de début"/>
            <TextField fx:id="dateFinUpdate" layoutX="100" layoutY="100" promptText="Date de fin"/>
            <TextField fx:id="typeUpdate" layoutX="100" layoutY="140" promptText="Type"/>
            <TextField fx:id="descriptionUpdate" layoutX="100" layoutY="180" promptText="Description"/>
            <Button layoutX="150" layoutY="220" onAction="#modifierEvent" text="Modifier"/>
            <Button layoutX="250" layoutY="220" onAction="#naviguerAjout" text="Ajouter"/>
            <Button layoutX="350" layoutY="220" onAction="#naviguerExit" text="Retour"/>
        </children>



</AnchorPane>
